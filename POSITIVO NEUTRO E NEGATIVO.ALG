Algoritmo "semnome"
Var
   n:inteiro

Inicio
   escreva("Digite um número: ")
   leia(n)

   se(n > 0) entao
      escreva("O número é positivo! ")

   senao

      se(n = 0) entao
         escreva("O número é neutro! ")

      senao
         escreva("O número é negativo! ")

      fimse
   fimse

Fimalgoritmo
